                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/size_of_text_in_panel"
                android:text="@string/text_default_value_averageIncomes"
                android:gravity="end"
                />
        </LinearLayout>
        <include layout="@layout/divider2"
            />
        <LinearLayout
