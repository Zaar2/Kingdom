                android:gravity="end"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linLayout_average_income_people_8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="visible"
