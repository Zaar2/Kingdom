<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/linearLayout_panelOfIndicators"
    >
    <RelativeLayout
        android:id="@+id/relatLay_indicators"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <!--        <TextView-->
        <!--            android:id="@+id/tv_titleOf_layoutIndicators"-->
        <!--            android:layout_width="match_parent"-->
        <!--            android:layout_height="wrap_content"-->
        <!--            android:layout_marginBottom="0dp"-->
        <!--            android:text="@string/name_titleOf_layoutIndicators"-->
        <!--            android:textAlignment="center"-->
        <!--            android:textSize="@dimen/size_of_text_title_in_frame_of_statistics" />-->
    </RelativeLayout>
    <LinearLayout
        android:id="@+id/linLayout_indicators"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <LinearLayout
            android:id="@+id/linLayout_increaseInPopulation_01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="visible"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/name_indicators_increaseInPopulation_1"
                android:layout_marginEnd="@dimen/margin_in_frame_of_statistics"
                android:textSize="@dimen/size_of_text_in_panel"
                />
            <TextView
                android:id="@+id/increaseInPopulation_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/size_of_text_in_panel"
                android:text="@string/text_default_value_peoples"
                android:gravity="end"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linLayout_corruption_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="visible"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/name_indicators_mortality_2"
                android:layout_marginEnd="@dimen/margin_in_frame_of_statistics"
                android:textSize="@dimen/size_of_text_in_panel"
                />
            <TextView
                android:id="@+id/mortality_tv_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/size_of_text_in_panel"
                android:text="@string/text_default_value_corruption"
                android:gravity="end"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linLayout_migratoryIncreaseInPopulation_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="visible"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/name_indicators_migratoryIncreaseInPopulation_3"
                android:layout_marginEnd="@dimen/margin_in_frame_of_statistics"
                android:textSize="@dimen/size_of_text_in_panel"
                />
            <TextView
                android:id="@+id/migratoryIncreaseInPopulation_tv_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/size_of_text_in_panel"
                android:text="@string/text_default_value_money"
                android:gravity="end"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/LinLayout_num_of_army"
            android:visibility="visible"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/name_indicators_army_9"
                android:layout_marginEnd="@dimen/margin_in_frame_of_statistics"
                android:textSize="@dimen/size_of_text_in_panel"
                />

            <TextView
                android:id="@+id/num_of_army_tv_resource"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:textSize="@dimen/size_of_text_in_panel"
                android:text="@string/text_defaultValue"
                android:gravity="end"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linLayout_unemployed_person_11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="visible"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/name_indicators_unemployed_person_11"
