                android:textSize="@dimen/size_of_text_in_panel"
                />
            <TextView
                android:id="@+id/grainLoss_indicator_tv_6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/size_of_text_in_panel"
                android:text="@string/text_default_value_averageIncomes"
                android:gravity="end"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linLayout_agriculture_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="visible"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/name_indicators_productivity_5"
                android:layout_marginEnd="@dimen/margin_in_frame_of_statistics"
                android:textSize="@dimen/size_of_text_in_panel"
                />
            <TextView
