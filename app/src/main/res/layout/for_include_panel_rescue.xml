                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/name_indicators_personCanHandle_8"
                android:layout_marginEnd="@dimen/margin_in_frame_of_statistics"
                android:textSize="@dimen/size_of_text_in_panel"
                />
            <TextView
                android:id="@+id/personCanHandle_indicator_tv_8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/size_of_text_in_panel"
                android:text="@string/text_default_value_averageIncomes"
                android:gravity="end"
                />
        </LinearLayout>
        <include layout="@layout/divider2"
            />
        <LinearLayout
            android:id="@+id/linLayout_warriors_in_a_Raid_10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
